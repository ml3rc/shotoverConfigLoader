{
  "name": "extension",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "prepare": "npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile src/theme/smui-light.css -i src/theme",
    "smui-theme-dark": "smui-theme compile src/theme/smui.css -i src/theme/dark"
  },
  "devDependencies": {
    "@smui/button": "^8.0.3",
    "@smui/card": "^8.0.3",
    "@smui/circular-progress": "^8.0.3",
    "@smui/layout-grid": "^8.0.3",
    "@sveltejs/vite-plugin-svelte": "^6.1.1",
    "@tsconfig/svelte": "^5.0.4",
    "smui-theme": "^8.0.3",
    "svelte": "^5.38.1",
    "svelte-check": "^4.3.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.2"
  }
}
