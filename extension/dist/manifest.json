{
  "manifest_version": 3,
  "name": "Shotover Setting Loader",
  "version": "1.0.0",
  "description": "Save/load settings for Shotover web ui",
  "permissions": ["scripting", "downloads", "activeTab", "storage","tabs"],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["/assets/content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "/src/popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}